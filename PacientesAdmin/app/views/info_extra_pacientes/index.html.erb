<p id="notice"><%= notice %></p>

<h1>Listing Info Extra Pacientes</h1>

<table>
  <thead>
    <tr>
      <th>Estudiante</th>
      <th>Trabajador</th>
      <th>Lugar estudio trabajo</th>
      <th>Hijos</th>
      <th>Cant hijos</th>
      <th>Referencia trab social</th>
      <th>Ayuda alimentos</th>
      <th>Ayuda medicamento</th>
      <th>Fk id persona</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @info_extra_pacientes.each do |info_extra_paciente| %>
      <tr>
        <td><%= info_extra_paciente.estudiante %></td>
        <td><%= info_extra_paciente.trabajador %></td>
        <td><%= info_extra_paciente.lugar_estudio_trabajo %></td>
        <td><%= info_extra_paciente.hijos %></td>
        <td><%= info_extra_paciente.cant_hijos %></td>
        <td><%= info_extra_paciente.referencia_trab_social %></td>
        <td><%= info_extra_paciente.ayuda_alimentos %></td>
        <td><%= info_extra_paciente.ayuda_medicamento %></td>
        <td><%= info_extra_paciente.fk_id_persona %></td>
        <td><%= link_to 'Show', info_extra_paciente %></td>
        <td><%= link_to 'Edit', edit_info_extra_paciente_path(info_extra_paciente) %></td>
        <td><%= link_to 'Destroy', info_extra_paciente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Info extra paciente', new_info_extra_paciente_path %>
