Listing Personas 
<table class="table table-bordered table-striped table-condensed">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Primer apellido</th>
      <th>Segundo apellido</th>
      <th>Fecha nacimiento</th>
      <th>Correo electronico</th>
      <th>Genero</th>
      <th>Cedula</th>
      <th>Estado civil</th>
      <th>Telefono</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personas.each do |persona| %>
      <tr>
        <td><%= persona.nombre %></td>
        <td><%= persona.primer_apellido %></td>
        <td><%= persona.segundo_apellido %></td>
        <td><%= persona.fecha_nacimiento %></td>
        <td><%= persona.correo_electronico %></td>
        <td><%= persona.genero %></td>
        <td><%= persona.cedula %></td>
        <td><%= persona.estado_civil %></td>
        <td><%= persona.telefono %></td>
        <td><%= link_to 'Show', persona %></td>
        <td><%= link_to 'Edit', edit_persona_path(persona) %></td>
        <td><%= link_to 'Destroy', persona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

